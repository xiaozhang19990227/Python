def sum(a,b):
    sum=a+b
    return sum

def calcomputer(num):
    result=0
    li=[]
    i=1
    while i<=num:
        result+=i
        i+=1
        pass
    li.append(result)
    return li
    pass

def returntuple():
    '''
    返回元组类型的数据
    :return:
    '''
    # return 1,2,3
    return{'name':'测试'}  #用大括号

print(sum(10,20))
value=calcomputer(10)
print(value)
print(type(value))
A=returntuple()
print(type(A))
